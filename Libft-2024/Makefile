name = libft.a
cc = gcc
srcs = $(wildcard *.c)
objects = $(srcs:.c=.o)
flags = -Wall -Wextra -Werror

ALL: $(name)

$(name): $(objects)
			ar rcs $(name) $(objects)

%.o: %.c
		$(cc) -c $(flags) $?

clean:
	rm -f $(objects)
fclean: clean
	rm -f $(name)
re: fclean all

.PHONY: all clean fclean re


