NAME = push_swap
cc = gcc
srcs = main.c push_swap.c
objects = $(srcs:.c=.o)
CFLAGS = -Wall -Wextra -Werror
rm = rm -f

all: $(NAME)

$(NAME): $(objects)
	$(cc) $(CFLAGS) -o $(NAME) $(objects)

clean:
	$(rm) $(objects)

fclean: clean
	$(rm) $(NAME)

re: fclean $(NAME)
	
.PHONY: all clean fclean re

