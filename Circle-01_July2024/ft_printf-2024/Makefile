NAME = libftprintf.a
cc = cc
srcs = ft_printf.c ft_printf-char.c ft_printf-num.c
objects = $(srcs:.c=.o)
CFLAGS = -Wall -Wextra -Werror
rm = rm -f

all: $(NAME)

$(NAME): $(objects)
	ar -rcs $(NAME) $(objects)

clean:
	$(rm) $(objects)

fclean: clean
	$(rm) $(NAME)

re: fclean $(NAME)

auto: all clean
	gcc main.c libftprintf.a

fuck: fclean
	$(rm) a.out
	
.PHONY: all clean fclean re

